<cfoutput>	<div class="article">		<h1>#prc.currentpage.PageTitle#</h1>				#prc.currentpage.PageText#	</div>	<div class="article">		<ul class="photolist">		<cfloop query="prc.galleries">		<cfquery name="pcount" datasource="#getSetting('datasource')#">				SELECT *				FROM Photos				Where GalleryID = <cfqueryparam value="#GalleryID#" >			</cfquery>		<li>			<a href="/photos/#prc.galleries.GLink#" title="#prc.galleries.GName# Photos">				<img src="/inc/images/#prc.galleries.GThumb#" width="300" class="shadow" title="#prc.galleries.GName# Photos" alt="#prc.galleries.GName# Photos" />				#prc.galleries.GName#<br>				<span style="font-size: -1;">#pcount.recordcount# photos</span>			</a>		</li>				</cfloop>		</ul>	</div></cfoutput>