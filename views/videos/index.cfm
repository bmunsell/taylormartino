<cfoutput><div class="content"><cfif IsDefined('rc.y') AND rc.y>	<cfif IsDefined('prc.i.stitle')><div class="vtitle"><strong>#prc.i.stitle#</strong></div></cfif>	<iframe class="youtube-player" type="text/html" width="640" height="442" src="http://www.youtube.com/embed/#rc.z#?autoplay=#rc.p#&hl=en_US&feature=player_embedded&version=3&rel=0" frameborder="0">	</iframe><cfelse>	<div id="slideshow">		<div id="controls">    		<span><a href="" id="prev">Prev</a></span>    		<span><a href="" id="next">Next</a></span>    		<span><a href="" id="pause">Pause</a></span>    		<span><a href="" id="play">Play</a></span>		</div>		<div id="slides" class="pics">		<cfloop query="prc.user" startrow="1" endrow="4">			<cfset v = Right(prc.user.id, 11)>       		<div id="" class="simg">    			<div class="vtitle2"><a href="#CGI.SCRIPT_NAME#?z=#v#&p=1&y=1"><strong>#prc.user.title#</strong></a></div>        		<div class="vplay"><a href="#CGI.SCRIPT_NAME#?z=#v#&p=1&y=1"><img src="images/btn-vplay.png" width="50" height="17" alt="Play Video"></a></div>      			<a href="#CGI.SCRIPT_NAME#?z=#v#&p=1&y=1"><img src="#prc.user.thumbnail_url#" width="640" height="480" border="0" class="grey" /></a>    		</div>		</cfloop>		</div>	</div> </cfif>  	<br>	<div class="clearfloat"></div>	<div>		<ul id="ytcarousel" class="jcarousel-skin-yt">		<cfloop query="prc.user" startrow="1" endrow="#prc.user.recordcount#">			<cfset v = Right(prc.user.id, 11)><!---#v#<br />--->			<li><a href="#CGI.SCRIPT_NAME#?z=#v#&p=1&y=1"><img src="#prc.user.thumbnail_url#" width="122" border="0" /><br /><h2>#prc.user.TITLE#</h2></a></li>		</cfloop>		</ul>	</div>	<div class="clearfloat"></div></div></cfoutput>